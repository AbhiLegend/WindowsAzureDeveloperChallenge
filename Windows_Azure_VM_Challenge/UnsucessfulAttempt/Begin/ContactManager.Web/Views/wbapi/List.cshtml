@model IEnumerable<ContactManager.Web.Models.wbapi>

@{
    ViewBag.Title = "List";
}
<section class="Featured">
    <div class="Title">
        <h1>The WorldBank CO2 emission LIst</h1>
<h2>Implemented for Windows Azure Websites</h2>
<h2>List</h2>
              <p>
          Create and search for contacts using Full-Text Search in SQL Server 2012 - try queries like &quot;Joh*&quot; (with the quotation marks) and john OR smith OR contoso
      </p>

    </div>
</section>


   


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CountryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CountryCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nineteensixty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.twothousandtwelve)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CountryCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nineteensixty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.twothousandtwelve)
        </td>
        
    </tr>
}
    @if (Model.Count() == 0)
{
     <tr>
          <td colspan="10">
                No contacts found
          </td>
     </tr>
}


</table>
